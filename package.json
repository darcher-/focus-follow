    "stylelint": "14.14.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "29.0.0",
    "stylelint-order": "5.0.0",
    "stylelint-prettier": "2.0.0",
    "tsutils": "3.21.0",
    "urix": "0.1.0",
    "uuid": "9.0.0"
  },
  "lint-staged": {
    "*.{js,ts,mjs,cjs,jsx,tsx,xml,svg,json,html}": "prettier-eslint --write",
    "*.{css,scss,sass,less}": "stylelint --fix",
    "*": "git add -A"
  },
  "license": "UNLICENSED",
  "name": "focus-follow",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/darcher-/focus-follow.git"
  },
  "scripts": {
    "git:logs": "git log --pretty=\"- %s\" > CHANGELOG.md",
    "hook:pre": "husky install",
    "lint": "yarn run lint:js && yarn run lint:css",
    "lint:css": "stylelint --fix \"**/*.{css,scss,sass,less}\"",
    "lint:js": "prettier-eslint --write --ignore-unknown \"**/*.{js,ts,jsx,tsx,mjs,cjs,svg,json,html}\"",
    "lint:view": "eslint . && prettier-eslint --list-different --ignore-unknown \"**/*.{js,ts,jsx,tsx,mjs,cjs,svg,json,html}\"",
    "test": "yarn run test:jest && yarn run test:badge",
    "test:badge": "yarn run test:jest && jest-coverage-badge",
    "test:cy": "cypress open",
    "test:jest": "jest --coverage --no-cache"
  },
  "type": "module",
  "version": "1.0.0"
}
